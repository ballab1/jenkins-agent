    <org.jenkinsci.plugins.workflow.job.properties.PipelineTriggersJobProperty>
      <triggers>
        <org.jenkinsci.plugins.gwt.GenericTrigger plugin="generic-webhook-trigger@1.88.1">
          <spec/>
          <genericVariables>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>BRANCH_REF</key>
              <value>$.pull_request.base.ref</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>PR_action</key>
              <value>$.action</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>PR_number</key>
              <value>$.number</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>PR_state</key>
              <value>$.pull_request.state</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>PR_URL</key>
              <value>$.pull_request.html_url</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
            <org.jenkinsci.plugins.gwt.GenericVariable>
              <expressionType>JSONPath</expressionType>
              <key>PR_COMMENTS</key>
              <value>$.pull_request.comments_url</value>
            </org.jenkinsci.plugins.gwt.GenericVariable>
          </genericVariables>
          <regexpFilterText>${PR_action}_${PR_state}_${BRANCH_REF}</regexpFilterText>
          <regexpFilterExpression>closed_closed_(staging|main)</regexpFilterExpression>
          <genericHeaderVariables>
            <org.jenkinsci.plugins.gwt.GenericHeaderVariable>
              <key>x_github_event</key>
              <regexpFilter>pull_request</regexpFilter>
            </org.jenkinsci.plugins.gwt.GenericHeaderVariable>
          </genericHeaderVariables>
          <printPostContent>false</printPostContent>
          <printContributedVariables>false</printContributedVariables>
          <causeString>Triggered by a merge to ${BRANCH_REF} by #PR-${PR_number}: ${PR_URL}</causeString>
          <token>private-token</token>
          <tokenCredentialId/>
          <silentResponse>false</silentResponse>
          <overrideQuietPeriod>false</overrideQuietPeriod>
          <shouldNotFlattern>false</shouldNotFlattern>
          <allowSeveralTriggersPerBuild>false</allowSeveralTriggersPerBuild>
        </org.jenkinsci.plugins.gwt.GenericTrigger>
      </triggers>
